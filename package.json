{
    "name": "sensor-data-logger-monorepo",
    "version": "1.0.0",
    "private": true,
    "workspaces": [
        "backend",
        "frontend"
    ],
    "scripts": {
        "start:backend": "npm start --prefix backend",
        "start:frontend": "npm start --prefix frontend",
        "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\""
    },
    "devDependencies": {
        "concurrently": "^7.0.0"
    }
}